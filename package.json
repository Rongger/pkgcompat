{
  "name": "pkgcompat",
  "version": "1.0.2",
  "description": "CLI tool to find npm package versions compatible with the current Node.js version",
  "main": "index.js",
  "bin": {
    "pkgcompat": "./index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Rongger/pkgcompat"
  },
  "scripts": {
    "prepare": "husky install"
  },
  "keywords": [
    "node",
    "package",
    "compatibility",
    "semver",
    "cli"
  ],
  "license": "MIT",
  "dependencies": {
    "commander": "^7.2.0",
    "package-json": "^6.5.0",
    "semver": "^7.7.2"
  },
  "devDependencies": {
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pnpm lint-staged"
    }
  },
  "lint-staged": {
    "*.{js?(x),ts?(x)}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{less,css,md,json}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=10"
  }
}
